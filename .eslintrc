{
	"extends": [
		"eslint:recommended",
		"plugin:import/errors",
		"plugin:react/recommended",
		"prettier",
		"prettier/react"
	],
	"rules": {
		"react/prop-types": 0,
		"semi": ["error", "always"]
	},
	"parser": "babel-eslint",
	"plugins": ["react", "import"],
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	}
}
